{
    "name": "pantheon-upstreams/drupal-composer-managed",
    "description": "Install Drupal 9 with Composer on Pantheon.",
    "type": "project",
    "license": "MIT",
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        },
        {
            "type": "path",
            "url": "upstream-configuration"
        }
    ],
    "require": {
        "pantheon-upstreams/upstream-configuration": "dev-main",
        "composer/installers": "^1.9",
        "drupal/core-composer-scaffold": "^10",
        "drupal/core-recommended": "^10",
        "pantheon-systems/drupal-integrations": "^10",
        "cweagans/composer-patches": "^1.7",
        "drush/drush": "^12",
        "drupal/paragraphs": "^1.15",
        "drupal/backup_migrate": "^5.0",
        "drupal/video_embed_field": "^2.5",
        "drupal/profile": "^1.7",
        "drupal/captcha": "^1.9",
        "drupal/twitter_block": "^3.0",
        "drupal/stage_file_proxy": "^2.0",
        "drupal/social_media_links": "^2.9",
        "drupal/cheeseburger_menu": "^5.0@RC",
        "drupal/webform": "^6.2@beta",
        "drupal/social_auth_google": "^4.0",
        "drupal/social_auth": "^4.0",
        "drupal/social_api": "^4.0",
        "drupal/google_api_client": "^4.3",
        "drupal/googlelogin-googlelogin": "^9.2",
        "drupal/quicktabs": "^3.0@alpha",
        "drupal/smtp": "^1.2",
        "drupal/persistent_login": "^2.0",
        "drupal/forgot_password_block": "^2.1",
        "drupal/views_accordion": "^2.0",
        "drupal/conditional_fields": "^4.0",
        "drupal/select2": "^1.15",
        "oomphinc/composer-installers-extender": "^2.0",
        "npm-asset/select2": "^4.0",
        "drupal/pathauto": "^1.11",
        "acquia/blt": "^13.7",
        "drupal/gin": "^3.0@RC",
        "pantheon-systems/upstream-management": "^1.0",
        "drupal/admin_toolbar": "^3.4",
        "drupal/tome": "^1.12",
        "drupal/simple_sitemap": "^4.1",
        "drupal/metatag": "^2.0",
        "drupal/devel": "^5.3",
        "drupal/views_data_export": "^1.4",
        "drupal/konfhub": "^1.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "patches": {
            "drupal/core": {
                "3463875 - BigPipe error - An error occurred during the execution of the Ajax response: LoadJS": "https://www.drupal.org/files/issues/2024-07-25/3463875-2.ajaxjs-prevent-loadjs-errors.patch"
            },
            "drupal/quicktabs": {
                "View's Empty Behavior not displayed anymore": "https://www.drupal.org/files/issues/2022-03-08/issues-3197108-fix-views-empty-behavior-not-displayed-error_0.patch"
            }
        },
        "drupal-scaffold": {
            "locations": {
                "web-root": "./docroot"
            },
            "allowed-packages": ["pantheon-systems/drupal-integrations"],
            "file-mapping": {
                "[project-root]/.editorconfig": false,
                "[project-root]/pantheon.upstream.yml": false,
                "[project-root]/.gitattributes": false
            }
        },
        "installer-paths": {
            "docroot/core": ["type:drupal-core"],
            "docroot/libraries/{$name}": [
                "type:drupal-library",
                "type:bower-asset",
                "type:npm-asset"
            ],
            "docroot/modules/contrib/{$name}": ["type:drupal-module"],
            "docroot/profiles/contrib/{$name}": ["type:drupal-profile"],
            "docroot/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/contrib/{$name}": ["type:drupal-drush"],
            "docroot/modules/custom/{$name}": ["type:drupal-custom-module"],
            "docroot/profiles/custom/{$name}": ["type:drupal-custom-profile"],
            "docroot/themes/custom/{$name}": ["type:drupal-custom-theme"],
            "docroot/private/scripts/quicksilver/{$name}/": ["type:quicksilver-script"]
        },
        "installer-types": [
            "bower-asset",
            "npm-asset"
        ],
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "enable-patching": true
    },
    "autoload": {
        "classmap": ["upstream-configuration/scripts/ComposerScripts.php"]
    },
    "scripts": {
        "pre-update-cmd": ["DrupalComposerManaged\\ComposerScripts::preUpdate"],
        "post-update-cmd": ["DrupalComposerManaged\\ComposerScripts::postUpdate"]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": false,
        "platform": {
            "php": "8.3"
        },
        "allow-plugins": {
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "drupal/core-composer-scaffold": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "phpstan/extension-installer": true,
            "oomphinc/composer-installers-extender": true,
            "acquia/blt": true,
            "php-http/discovery": true,
            "pantheon-systems/upstream-management": true
        }
    },
    "require-dev": {
        "drupal/core-dev": "^10.3"
    }
}
